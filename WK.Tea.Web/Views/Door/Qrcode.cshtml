<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>倒茶</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <link rel="stylesheet" href="~/assets/layui/css/layui.css" media="all">
    <style type="text/css">
        html, body {
            height:100%;
        }
    </style>
</head>
<body>
    @if (ViewBag.qcode != null)
    {
        <img src="@ViewBag.qcode"/>
    }

    @if (ViewBag.Url != null)
    {
        <iframe style="border:0; height:100%;width:100%" src="@ViewBag.Url"></iframe>
    }
    else
    {
        <div style="padding:50px 15px; line-height:30px; font-size:16px;">
            <pre>@ViewBag.Message</pre>
        </div>
    }
</body>
</html>
